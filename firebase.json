{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "package.json",
      "package-lock.json",
      "/src"
    ],
    "appAssociation": "AUTO",
    "rewrites": [ { "source": "/link/**", "dynamicLinks": true } ]
  }
}
