---
    <h1>Code Projects</h1>
    <div class="split-card" id="freeperiodictables">
      <div class="pics">
          <img src="/images/code/freeperiodictables.png" alt="Free Periodic Tables">
      </div>
      <div class="text">
          <h3>Free Periodic Tables</h3>
          <p><a href="https://11ty.dev">11ty</a>, Figma</p>
          <a class="button" href="https://periodictables.zacharyc.site" target="_blank">Visit</a>
      </div>
    </div>
    <div class="split-card" id="wordle-hacker">
      <div class="pics">
          <img src="/images/code/wordle-hacker.png" alt="Wordle Hacker">
      </div>
      <div class="text">
          <h3>Wordle Hacker</h3>
          <p>Chrome Extention</p>
          <a class="button" href="https://github.com/ZacharyCrespin/wordle-hacker-chrome-extension" target="_blank">Download</a>
      </div>
    </div>
    <div class="collections">
      {%- for code in collections.code | reverse -%}
      <div class="card">
        <h3>{{ code.data.title }}</h3>
        <p>{{ code.data.tech }}</p>
        <a class="button" href="{{ code.data.link }}" target="_blank">{{ code.data.linktext }}</a>
      </div>
      {%- endfor -%}
    </div>