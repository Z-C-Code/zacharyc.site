<h1>Photography</h1>
<h2>Latest Collections</h2>
<div class="collections">
  {%- for collection in collections.collection | reverse | limit(4) -%}
  <div class="card">
    <img src="/images/banners/{{ collection.data.id }}.jpg" alt="{{ collection.data.title }} Collection Image" width="100%">
    <h3>{{ collection.data.title }}</h3>
    <p>{{ collection.data.numberofphotos }} Photos</p>
    <a class="button" href="{{ collection.url }}">View</a>
  </div>
  {%- endfor -%}
</div>
<a class="button" href="/photography/collections">All Collections</a>
