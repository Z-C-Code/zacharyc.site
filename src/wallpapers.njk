---
title: Wallpapers
description: Mobile Wallpapers - Free Downloads
id: wallpapers
layout: default
extraHeadCode: | 
  <!-- Pinterest Tag -->
  <script>
    !function(e){if(!window.pintrk){window.pintrk = function () {
    window.pintrk.queue.push(Array.prototype.slice.call(arguments))};var
      n=window.pintrk;n.queue=[],n.version="3.0";var
      t=document.createElement("script");t.async=!0,t.src=e;var
      r=document.getElementsByTagName("script")[0];
      r.parentNode.insertBefore(t,r)}}("https://s.pinimg.com/ct/core.js");
    pintrk('load', '2612840119152');
    pintrk('page');
    pintrk('track', 'pagevisit', {
      property: 'Wallpapers'
    });
  </script>
  <noscript>
  <img height="1" width="1" style="display:none;" alt=""
  src="https://ct.pinterest.com/v3/?event=init&tid=2612840119152&noscript=1" />
  </noscript>
  <!-- end Pinterest Tag -->
  <!-- Download -->
  <script>
    function download(file) {
      // Analytics
      instance.action('3d80f3b3-edb5-403c-9307-2ece59b5509a', { key: file, value: 1 })

      // Pinterest
      pintrk('track', 'checkout', {
        value: 0.00,
        order_quantity: 1,
        currency: 'USD',
        line_items: [
          {
            product_name: file,
            product_price: 0.00,
            product_quantity: 1
          }
        ]
      });

      // Initiate file download
      var link = document.createElement('a');
      link.href = file;
      link.download = file;
      link.click();
    };
  </script>
---
{% for wallpaper in collections.wallpaper | sort('order') | reverse %}
<div class="card full" id="{{ wallpaper.data.id }}" aria-label="{{ wallpaper.data.name }}">
  {% image "wallpapers/" + wallpaper.data.id + ".png", wallpaper.data.name + " phone wallpapers", 300, true %}
  <div class="text">
    <h2 class="h3">{{ wallpaper.data.name }}</h2>
    <div class="tags">
      {% for tag in wallpaper.data.tags | filterTags %}
        <p class="tag">{{ tag }}</p>
      {% endfor %}
    </div>
    <button onclick="download('/files/{{- wallpaper.data.id -}}.zip')">Download</button>
  </div>
</div>
{% endfor %}
